{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine",
      "node"
    ],
    "baseUrl": "./",
    "paths": {
      "@models/*": ["./src/app/models/*"],
      "@routes/*": ["./src/app/services/routes/*"],
      "@services/*": ["./src/app/services/ui/*"]
    }
  },
  "files": [
    "src/test.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ]
}
